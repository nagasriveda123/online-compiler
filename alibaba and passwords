#alibaba and passwords
digit=input("enter string")
n=int(input())
passwords=list(map(str,input() .split(' ')))[:n]
count=0
l1=list(digit)
print(passwords)
for i in passwords:
    c=1
    for j in i:
        if j not in l1:
            c=0
            break
    if c:
        count=count+1
print(count)
